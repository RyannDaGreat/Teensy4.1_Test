import gc


#If nothing happens, all three of these should print the same value
print("PART 1: Should be same")
print(gc.mem_free())
print(gc.mem_free())
print(gc.mem_free())

print("PART 2: Should decrease linearly")
print(gc.mem_free())
a={}
print(gc.mem_free())
b={}
print(gc.mem_free())
c={}
print(gc.mem_free())

print("PART 3: Should stay the same")
a=10
print(gc.mem_free())
a=10
print(gc.mem_free())
a=11
print(gc.mem_free())
a=1999
print(gc.mem_free())
